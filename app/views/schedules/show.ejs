<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.schedules %>">schedules</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">Details of schedule</a></li>
</ul>

<div class="page-header">
    <h1>Details of schedule</h1>
</div>

<table class="table table-bordered">
    <tbody>
        <tr><th>uid</th><td><%= schedule.uid %></td></tr>

        <tr><th>offerings</th><td><%= schedule.offerings %></td></tr>
    </tbody>
</table>

    <div class="container">
     <div class="row">
      <div class="span4" id="sidenav">
       <div class="searchbar">
        <form class="navbar-form pull-left">
            <input type="department" id="searchform" class="span2" style="margin-top:0">
            <input type="courseno" id="searchform" class="span1" style="margin-top:0">
            <button type="submit" id="searchbtn" class="btn" style="margin-left:0" onclick="return doQuery('professor:ord')">Submit</button>
        </form>
        <div class="results">
        </div>
       </div>
     </div>
     <div class="span8" id="tables" >
     </div>
   </div>
 </div>
 <script type="text/javascript">
    $(startAutoComplete());
