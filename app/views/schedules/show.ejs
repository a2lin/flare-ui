 // $(coverElement($("div.tablestart:contains('07:00')")[0], $("div.tablestart:contains('07:30')")[0]));
 // assignDocListeners();

 </script>

<div class="well" style="margin-top:10px">
    <%- link_to('<i class="icon-edit icon-white"></i> Edit', pathTo.edit_schedule(schedule), {class: 'btn btn-primary'}) %>
    <%- link_to('<i class="icon-remove icon-white"></i> Delete', pathTo.schedule(schedule), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>
    or
    <%- link_to('Back to index', pathTo.schedules) %>
</div>
